    "symfony/finder": "^2.6.0",
    "symfony/security-bundle": "^2.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.*",
    "symfony/framework-bundle": "^2.6.0",
    "symfony/browser-kit": "^2.6.0",
    "symfony/form": "^2.6.0"
